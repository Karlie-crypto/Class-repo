#include <stdio.h>
/**
 * Main - main entry
 * Description: A program that shows a function without arguments and no return value
 * Return void
 */
void noArguments(void);
void main()
{
   noArguments(); 
}
void noArguments(void)
{
  int a = 10, b = 5, noArguments = 0;
  noArguments = a + b;
  printf("noArguments = %d\n", noArguments);

}  
  
